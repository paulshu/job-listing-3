<div class="col-md-6 col-centered">
  <div class="panel panel-info">
    <div class="panel-heading">
      <h3>用户信息</h3>
    </div>
    <div class="panel-footer">
      <table class="table table-boldered">
          <thead>
              <tr>
                  <td>用户名</td>
                  <td>注册邮箱</td>
              </tr>

          </thead>
          <tbody>
              <% @users.each do |user| %>
                <tr>
                  <td> <%= current_user.name  %></td>
                  <td><%= current_user.email %></td>
                </tr>
              <% end %>
          </tbody>
        </table>
      </div>
    </div>
</div>
